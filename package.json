{
  "name": "make-starter",
  "version": "1.0.0",
  "description": "Makefile starter with common tasks and scenarios",
  "repository": {
    "type": "git",
    "url": "git://github.com/nhunzaker/make-starter.git"
  },
  "license": "MIT",
  "scripts": {
    "start": "make watch",
    "test": "npm test",
    "postinstall": "make all -j8"
  },
  "babel": {
    "stage": 1,
    "loose": true,
    "optional": [
      "utility.inlineEnvironmentVariables"
    ]
  },
  "dependencies": {
    "autoprefixer": "~5.2",
    "babel-core": "~5",
    "babel-loader": "~5",
    "babel-plugin-object-assign": "~1.2",
    "node-sass": "~3.2",
    "postcss-cli": "~1.5",
    "swig": "~1.4",
    "webpack": "~1.11"
  },
  "devDependencies": {
    "chokidar": "^1.0.5",
    "karma": "~> 0.13.8",
    "karma-cli": "~0.1",
    "karma-firefox-launcher": "~0.1",
    "karma-mocha": "~0.2",
    "karma-mocha-reporter": "~1.1",
    "karma-sinon-chai": "~1.0",
    "karma-webpack": "~1",
    "mocha": "~2.2",
    "browser-sync": "~2.8"
  }
}
